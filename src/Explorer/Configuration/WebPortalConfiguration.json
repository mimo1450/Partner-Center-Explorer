  "Plugins": {
    "Defaults": {
      "DisplayName": "Unknown",
      "Tile": "icon fa fa-question",
      "Color": "#3D3C3A",
      "AlternateColor": "#4D4C4A"
    },
    "Commons": {
      "Name": "Commons",
      "DefaultAssetVersion": "1.0",
